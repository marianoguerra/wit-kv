package test:types;

interface types {
    record point {
        x: u32,
        y: u32,
    }

    record person {
        age: u8,
        score: u32,
    }

    enum color {
        red,
        green,
        blue,
    }

    variant shape {
        circle(u32),
        rectangle(point),
        none,
    }

    type maybe-u32 = option<u32>;

    type result-u32 = result<u32, u8>;

    flags permissions {
        read,
        write,
        execute,
    }
}
